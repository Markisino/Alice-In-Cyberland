[32m+[m[32m﻿using System.Collections;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class EnemyTurret : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    //EnemyMovement[m
[32m+[m[32m    //private Animator animator;[m
[32m+[m[32m    public float speed;[m
[32m+[m[32m    public float rotatingSpeed;[m
[32m+[m[32m    private Transform target;[m
[32m+[m
[32m+[m[32m    //EnemyAttack[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    GameObject bullet;[m
[32m+[m[32m    public float firingRate;[m
[32m+[m[32m    public float nextShot;[m
[32m+[m[32m    public float timer;[m
[32m+[m
[32m+[m[32m    //Explosion[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    //GameObject ExplosionPrefab;[m
[32m+[m
[32m+[m[32m    PlayerControl playerContScript;[m
[32m+[m[32m    private Rigidbody2D rb2d;       //Store a reference to the Rigidbody2D component required to use 2D Physics.[m
[32m+[m[32m    int enemyHP = 4;[m
[32m+[m[41m    [m
[32m+[m[32m    //Getter and setter to get players position[m
[32m+[m[32m    public Transform Target[m
[32m+[m[32m    {[m
[32m+[m[32m        get[m
[32m+[m[32m        {[m
[32m+[m[32m            return target;[m
[32m+[m[32m        }[m
[32m+[m[32m        set[m
[32m+[m[32m        {[m
[32m+[m[32m            target = value;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Use this for initialization[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m        //Added[m
[32m+[m[32m        GameObject thePlayer = GameObject.Find("Aris");[m
[32m+[m[32m        playerContScript = thePlayer.GetComponent<PlayerControl>();[m
[32m+[m[32m        firingRate = 1f;[m
[32m+[m[32m        nextShot = Time.time;[m[41m    [m
[32m+[m[32m        rb2d = GetComponent<Rigidbody2D>();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update is called once per frame[m
[32m+[m[32m    void FixedUpdate()[m
[32m+[m[32m    {[m
[32m+[m[32m        timer += Time.deltaTime;[m
[32m+[m
[32m+[m[32m        if (target != null && (playerContScript.current_hp != 0 || playerContScript.current_hp < 0))[m
[32m+[m[32m        {[m
[32m+[m[32m            //If there is a target it will rotate towards it + 90deg makes it face the player[m[41m [m
[32m+[m[32m            float rotationDeg = Mathf.Atan2(transform.position.y - target.transform.position.y, transform.position.x - target.transform.position.x) * Mathf.Rad2Deg + 90f;[m
[32m+[m[32m            transform.eulerAngles = new Vector3(0f, 0f, Mathf.MoveTowardsAngle(transform.eulerAngles.z, rotationDeg, Time.deltaTime * rotatingSpeed));[m
[32m+[m[32m            shoot();[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (target == null)[m
[32m+[m[32m        {[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public int getEnemyHP()[m
[32m+[m[32m    {[m
[32m+[m[32m        return enemyHP;[m
[32m+[m[32m    }[m
[32m+[m[32m    public void reduceEnemyHP()[m
[32m+[m[32m    {[m
[32m+[m[32m        reduceEnemyHP(1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void reduceEnemyHP(int value)[m
[32m+[m[32m    {[m
[32m+[m[32m        enemyHP -= value;[m
[32m+[m[32m        if (enemyHP <= 0)[m
[32m+[m[32m            Die();[m
[32m+[m[32m    }[m
[32m+[m[32m    public void Die()[m
[32m+[m[32m    {[m
[32m+[m[32m        Destroy(gameObject);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void shoot()[m
[32m+[m[32m    {[m
[32m+[m[32m        if(Time.time > nextShot)[m
[32m+[m[32m        {[m
[32m+[m[32m            nextShot = Time.time + firingRate;[m
[32m+[m[32m            Instantiate(bullet, transform.position, Quaternion.identity);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    void OnTriggerEnter2D(Collider2D other)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (other.gameObject.layer == 9)//alice tools[m
[32m+[m[32m        {[m
[32m+[m[32m              AliceWeapon aw = other.gameObject.GetComponent<AliceWeapon>();[m
[32m+[m[32m            reduceEnemyHP(aw.damage);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m

[32m+[m[32m﻿using System.Collections;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class Bomber : MonoBehaviour {[m
[32m+[m
[32m+[m[32m    //EnemyMovement[m
[32m+[m[32m    private Animator animator;[m
[32m+[m[32m    public float speed;[m
[32m+[m[32m    private Transform target;[m
[32m+[m[32m    protected Vector2 direction;[m
[32m+[m
[32m+[m[32m    //EnemyAttack[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    GameObject bullet;[m
[32m+[m[32m    public float firingRate;[m
[32m+[m[32m    public float nextShot;[m
[32m+[m[32m    public float timer;[m
[32m+[m
[32m+[m[32m    //RandomWalk Script[m
[32m+[m[32m    EnemyRandomWalk EnemyRngWalk;[m
[32m+[m
[32m+[m[32m    //Explosion[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    GameObject ExplosionPrefab;[m
[32m+[m
[32m+[m[32m    private PlayerControl playerContScript;[m
[32m+[m
[32m+[m[32m    private Rigidbody2D rb2d;       //Store a reference to the Rigidbody2D component required to use 2D Physics.[m
[32m+[m[32m    int enemyHP = 5;[m
[32m+[m[32m    //Getter and setter to get players position[m
[32m+[m[32m    public Transform Target[m
[32m+[m[32m    {[m
[32m+[m[32m        get[m
[32m+[m[32m        {[m
[32m+[m[32m            return target;[m
[32m+[m[32m        }[m
[32m+[m[32m        set[m
[32m+[m[32m        {[m
[32m+[m[32m            target = value;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Use this for initialization[m
[32m+[m[32m    void Start () {[m
[32m+[m[32m        //Added[m
[32m+[m[32m        GameObject thePlayer = GameObject.Find("Aris");[m
[32m+[m[32m        playerContScript = thePlayer.GetComponent<PlayerControl>();[m
[32m+[m[32m        animator = GetComponent<Animator>();[m
[32m+[m[32m        firingRate = 1f;[m
[32m+[m[32m        nextShot = Time.time;[m
[32m+[m[32m        EnemyRngWalk = GetComponent<EnemyRandomWalk>();[m
[32m+[m[32m        rb2d = GetComponent<Rigidbody2D>();[m
[32m+[m[32m    }[m
[32m+[m[41m	[m
[32m+[m	[32m// Update is called once per frame[m
[32m+[m	[32mvoid FixedUpdate () {[m
[32m+[m[32m        animatedDirection(direction);[m
[32m+[m[32m        timer += Time.deltaTime;[m
[32m+[m
[32m+[m[32m        if (target != null)[m
[32m+[m[32m        {[m
[32m+[m[32m            //direction = Vector2.zero;[m
[32m+[m[32m            RunToTarget();[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (target == null)[m
[32m+[m[32m        {[m
[32m+[m[32m           EnemyRngWalk.FollowRandomDirection();[m
[32m+[m[32m        }[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m    //Set animation for all 4 direction using animator and variable[m
[32m+[m[32m    private void animatedDirection(Vector2 direction)[m
[32m+[m[32m    {[m
[32m+[m[32m        animator.SetFloat("x", direction.x);[m
[32m+[m[32m        animator.SetFloat("y", direction.y);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //When the player is collided with the enemy it will run the opposite way[m
[32m+[m[32m    private void RunToTarget()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (target != null)[m
[32m+[m[32m        {[m
[32m+[m[32m            direction = (target.transform.position - transform.position).normalized;[m
[32m+[m[32m            rb2d.velocity = (direction)*speed*Time.deltaTime;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public int getEnemyHP()[m
[32m+[m[32m    {[m
[32m+[m[32m        return enemyHP;[m
[32m+[m[32m    }[m
[32m+[m[32m    public void reduceEnemyHP()[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m        reduceEnemyHP (1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void reduceEnemyHP(int value){[m
[32m+[m[32m        enemyHP -= value;[m
[32m+[m[32m        if(enemyHP <=0)[m
[32m+[m[32m        Die();[m
[32m+[m[32m    }[m
[32m+[m[32m    public void Die(){[m
[32m+[m[32m        Instantiate(ExplosionPrefab, transform.position, Quaternion.identity);[m
[32m+[m
[32m+[m[32m        Destroy(gameObject);[m
[32m+[m[32m    }[m
[32m+[m[32m    void OnCollisionEnter2D(Collision2D other){[m
[32m+[m[32m        if(other.gameObject.layer ==9)//alice tools[m
[32m+[m[32m        {[m
[32m+[m[32m            AliceWeapon aw = other.gameObject.GetComponent<AliceWeapon>();[m
[32m+[m[32m            reduceEnemyHP(aw.damage);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if(other.gameObject.tag == "Player")[m
[32m+[m[32m        {[m
[32m+[m[32m            Instantiate(ExplosionPrefab, transform.position, Quaternion.identity);[m
[32m+[m[32m                        playerContScript.takeDamage();[m[41m      [m
[32m+[m
[32m+[m[32m            Destroy(transform.parent.gameObject);[m
[32m+[m[32m        }[m[41m [m
[32m+[m[32m    }[m
[32m+[m[32m}[m

[32m+[m[32m    {[m
[32m+[m[32m        if (col.tag == "Player")[m
[32m+[m[32m        {[m
[32m+[m[32m            parent.Target = col.transform;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void OnTriggerExit2D(Collider2D col)[m
[32m+[m[32m    {[m
[32m+[m[32m        parent.Target = null;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m

[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class BomberRange : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    // Start is called before the first frame update [SerializeField] private Enemy parent;[m[41m [m
[32m+[m[32m   [SerializeField] private Bomber parent;[m[41m [m
[32m+[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m       // parent = GetComponentInChildren<Enemy>();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    void Update(){[m
[32m+[m[32m        transform.position = parent.transform.position;[m
[32m+[m[32m    }[m
[32m+[m	[32mprivate void OnTriggerEnter2D(Collider2D col)[m
[32m+[m[32m    {[m
[32m+[m[32m        if(col.tag == "Player")[m
[32m+[m[32m        {[m
[32m+[m[32m            parent.Target = col.transform;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m

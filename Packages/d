[32m+[m[32m﻿using System.Collections;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class EnemyBullet : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    float bulletSpeed = 5f;[m
[32m+[m[32m    private Rigidbody2D rb2d;[m
[32m+[m
[32m+[m[32m    PlayerControl target;[m
[32m+[m[32m    Vector2 direction;[m
[32m+[m
[32m+[m[32m    private PlayerControl playerContScript;[m
[32m+[m
[32m+[m
[32m+[m[32m    // Start is called before the first frame update[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m        rb2d = GetComponent<Rigidbody2D>();[m
[32m+[m[32m        target = GameObject.FindObjectOfType<PlayerControl>();[m
[32m+[m
[32m+[m[32m        GameObject thePlayer = GameObject.Find("Aris");[m
[32m+[m[32m        playerContScript = thePlayer.GetComponent<PlayerControl>();[m
[32m+[m
[32m+[m[32m        direction = (target.transform.position - transform.position).normalized * bulletSpeed;[m
[32m+[m[32m        rb2d.velocity = new Vector2(direction.x, direction.y);[m
[32m+[m[32m        Destroy(gameObject, 2f);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    void OnTriggerEnter2D(Collider2D other)[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m        if (other.gameObject.name.Equals("Aris"))[m
[32m+[m[32m        {[m
[32m+[m[32m            //Cannot manually set the damage taken[m
[32m+[m[32m            Debug.Log(playerContScript.current_hp);[m
[32m+[m[32m            playerContScript.takeDamage();[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m                    Destroy(gameObject);[m
[32m+[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
